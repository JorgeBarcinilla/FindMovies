{
  "name": "find-movies",
  "version": "1.0.0",
  "author": "Jorge Palacio Barcinilla",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:FindMovies": "node dist/FindMovies/server/server.mjs",
    "prepare": "husky install",
    "build:doc": "compodoc -p tsconfig.doc.json --theme  -d dist/documentation ",
    "lint": "ng lint",
    "storybook": "ng run FindMovies:storybook",
    "build-storybook": "ng run FindMovies:build-storybook",
    "netlify:deploy": "npm run build && npm run build:doc && cp -r dist/documentation dist/FindMovies/browser"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,html}": [
      "eslint --fix ",
      "prettier --write"
    ],
    "*.{html,css,scss,md,json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.13",
    "@netlify/angular-runtime": "^3.0.1",
    "@ngrx/operators": "^20.1.0",
    "@ngrx/signals": "^20.1.0",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2003.0",
    "@angular-devkit/build-angular": "^20.3.0",
    "@angular-devkit/core": "^20.3.0",
    "@angular-eslint/schematics": "^21.0.1",
    "@angular/build": "^20.3.13",
    "@angular/cli": "^20.3.13",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/platform-browser-dynamic": "^20.3.0",
    "@compodoc/compodoc": "^1.1.32",
    "@ngrx/eslint-plugin": "^20.0.1",
    "@ngrx/schematics": "^20.1.0",
    "@smarttools/eslint-plugin-rxjs": "^1.0.22",
    "@storybook/addon-a11y": "^10.1.4",
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-onboarding": "^10.1.4",
    "@storybook/angular": "^10.1.4",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "angular-eslint": "20.6.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^60.3.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-storybook": "^10.1.4",
    "husky": "^9.0.6",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "storybook": "^10.1.4",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.40.0",
    "zone.js": "^0.15.1"
  }
}
